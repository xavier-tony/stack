{
  "name": "stack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && husky install",
    "prepublishOnly": "pinst --disable",
    "postpack": "pinst --enable",
    "start": "nx serve",
    "serve:mock": "nx serve -c=mock",
    "build": "nx build",
    "test": "nx test && nx e2e",
    "test:unit": "jest --env=jest-environment-jsdom-sixteen",
    "test:e2e": "start-server-and-test start http://localhost:4200 cy:run",
    "source-map-explore": "source-map-explorer dist/apps/out-school/**/*.js",
    "explore": "run-s build source-map-explore",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "ngd": "npx ngd -p tsconfig.doc.json -o",
    "stats": "webpack-bundle-analyzer dist/apps/out-school/stats.json",
    "analyze": "run-s \"build -- --prod --stats-json\" stats",
    "profile": "NX_PROFILE=profile.json nx build $1",
    "start:api": "nx serve api-school",
    "start:api:debug": "nx serve api-school --inspect",
    "start:store": "nx serve datastore",
    "prepare": "husky install",
    "format": "nx format --all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.2",
    "@angular/cdk": "16.1.2",
    "@angular/common": "16.1.2",
